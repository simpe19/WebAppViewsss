@{
    ViewData["Title"] = "Produkter";
    var products = await productService.GetAllAsync();
}

@if (products != null) {
    foreach(var product in products)
    {
        <section class="Allproducts">
               <div class="container">
                <div class="mb-3">
                    <div>@product.Id</div>
                    <div>@product.Name</div>
                    <div>@product.Description</div>
                    <div>@product.Price</div>
                </div>
               </div>

        </section>

    }
}